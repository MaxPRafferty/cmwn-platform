{
  "name": "cmwn/platform",
  "description": "API layer for CMWN",
  "license": "proprietary",
  "version": "0.5.4",
  "minimum-stability": "stable",
  "config": {
    "process-timeout": 5000
  },
  "require": {
    "php": "~5.6 || ~7.0",
    "ext-redis": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "zendframework/zend-db": "~2.3",
    "zendframework/zend-eventmanager": "~2.3",
    "zendframework/zend-mvc": "~2.3",
    "zendframework/zend-paginator": "~2.3",
    "zendframework/zend-servicemanager": "~2.3",
    "zendframework/zend-stdlib": "~2.3",
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-cache": "~2.6",
    "zendframework/zend-config": "~2.6",
    "zendframework/zend-console": "~2.6",
    "zendframework/zend-file": "~2.6",
    "zendframework/zend-filter": "~2.6",
    "zendframework/zend-http": "~2.5",
    "zendframework/zend-hydrator": "~1.0",
    "zendframework/zend-i18n": "~2.6",
    "zendframework/zend-i18n-resources": "^2.5",
    "zendframework/zend-inputfilter": "^2.5",
    "zendframework/zend-json": "~2.6",
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-log": "~2.6",
    "zendframework/zend-mail": "~2.6",
    "zendframework/zend-permissions-rbac": "^2.5",
    "zendframework/zend-session": "~2.6",
    "zendframework/zend-serializer": "~2.6",
    "zendframework/zend-text": "~2.6",
    "zendframework/zend-uri": "^2.5",
    "zendframework/zend-validator": "~2.6",
    "zfcampus/zf-apigility": "1.2.0",
    "zfcampus/zf-hal": "1.3.1",
    "zfcampus/zf-apigility-documentation": "~1.0.5",
    "ramsey/uuid": "~3.0",
    "cloudinary/cloudinary_php": "~1.3",
    "acelaya/zf2-acmailer": "~5.0",
    "phpoffice/phpexcel": "~1.8",
    "chrisboulton/php-resque": "~1.2",
    "rollbar/rollbar": "v0.18.2",
    "aws/aws-sdk-php-zf2": "2.*",
    "robmorgan/phinx": "0.6.4",
    "zfcampus/zf-content-validation": "~1.0-dev"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "~2.0",
    "phpunit/php-token-stream": "1.4.9",
    "zfcampus/zf-development-mode": "~2.0",
    "squizlabs/php_codesniffer": "2.5.1",
    "phpmd/phpmd": "2.3.2",
    "zendframework/zftool": "dev-master",
    "zendframework/zend-developer-tools": "dev-master",
    "zfcampus/zf-apigility-admin": "1.4.1",
    "zfcampus/zf-apigility-admin-ui": "1.2.2",
    "zfcampus/zf-deploy": "~1.0",
    "fzaninotto/faker": "~1.5",
    "zendframework/zend-test": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "Api\\": "module\\Api\\src\\Api",
      "Application\\": "module\\Application\\src\\Application",
      "Asset\\": "module\\Asset\\src\\Asset",
      "Flip\\": "module\\Flip\\src\\Flip",
      "Flag\\": "module\\Flag\\src\\Flag",
      "Forgot\\": "module\\Forgot\\src\\Forgot",
      "Friend\\": "module\\Friend\\src\\Friend",
      "Game\\": "module\\Game\\src\\Game",
      "Group\\": "module\\Group\\src\\Group",
      "Import\\": "module\\Import\\src\\Import",
      "Job\\": "module\\Job\\src\\Job",
      "Media\\": "module\\Media\\src\\Media",
      "Notice\\": "module\\Notice\\src\\Notice",
      "Org\\": "module\\Org\\src\\Org",
      "Security\\": "module\\Security\\src\\Security",
      "Skribble\\": "module\\Skribble\\src\\Skribble",
      "Suggest\\": "module\\Suggest\\src\\Suggest",
      "User\\": "module\\User\\src\\User",
      "Sa\\": "module\\Sa\\src\\Sa"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module\\Application\\test\\ApplicationTest",
      "AssetTest\\": "module\\Asset\\test\\AssetTest",
      "FlipTest\\": "module\\Flip\\test\\FlipTest",
      "FlagTest\\": "module\\Flag\\test\\FlagTest",
      "ForgotTest\\": "module\\Forgot\\test\\ForgotTest",
      "FriendTest\\": "module\\Friend\\test\\FriendTest",
      "GameTest\\": "module\\Game\\test\\GameTest",
      "GroupTest\\": "module\\Group\\test\\GroupTest",
      "ImportTest\\": "module\\Import\\test\\ImportTest",
      "JobTest\\": "module\\Job\\test\\JobTest",
      "MediaTest\\": "module\\Media\\test\\MediaTest",
      "NoticeTest\\": "module\\Notice\\test\\NoticeTest",
      "OrgTest\\": "module\\Org\\test\\OrgTest",
      "SecurityTest\\": "module\\Security\\test\\SecurityTest",
      "SkribbleTest\\": "module\\Skribble\\test\\SkribbleTest",
      "SuggestTest\\": "module\\Suggest\\test\\SuggestTest",
      "UserTest\\": "module\\User\\test\\UserTest",
      "SaTest\\": "module\\Sa\\test\\SaTest",
      "IntegrationTest\\": "test\\IntegrationTest"
    }
  }
}
